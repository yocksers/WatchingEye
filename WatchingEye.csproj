<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<AssemblyName>WatchingEye</AssemblyName>
		<OutputType>Library</OutputType>

		<LangVersion>10.0</LangVersion>
		<Nullable>enable</Nullable>

		<GenerateAssemblyInfo>false</GenerateAssemblyInfo>
		<AssemblyVersion>1.5.4.0</AssemblyVersion>
		<FileVersion>1.5.4.0</FileVersion>
		<RootNamespace>WatchingEye</RootNamespace>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="ExternalAdmin.html" />
		<None Remove="ExternalAdmin.js" />
		<None Remove="Images\logo.jpg" />
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Include="Web\ExternalAdmin.html" />
		<EmbeddedResource Include="Web\ExternalAdmin.js" />
		<EmbeddedResource Include="Images\logo.jpg" />
		<EmbeddedResource Include="WatchingEyeConfiguration.html" />
		<EmbeddedResource Include="WatchingEyeConfiguration.js" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="MediaBrowser.Server.Core" Version="4.9.1.80" />
		<PackageReference Include="ServiceStack" Version="8.8.0" />
	</ItemGroup>


	<ItemGroup>
		<None Update="plugin.json">
			<CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
		</None>
	</ItemGroup>

</Project>